<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CoreBundle\Entity\CheckIn" table="check_in">
    <indexes>
      <index name="fk_check_in_vehicle1_idx" columns="vehicle"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lat" type="decimal" column="lat" precision="6" scale="2" nullable="false"/>
    <field name="long" type="decimal" column="long" precision="6" scale="2" nullable="false"/>
    <field name="timestamp" type="datetime" column="timestamp" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="vehicle" target-entity="Vehicle" fetch="LAZY">
      <join-columns>
        <join-column name="vehicle" referenced-column-name="license_plate_no"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
